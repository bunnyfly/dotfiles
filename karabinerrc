<?xml version="1.0"?>
<root>
  <item>
    <name>Row Up!</name>
    <appendix>Moves keyboard layout up one row.</appendix>
    <appendix>Bottom row becomes modifier keys.</appendix>
    <appendix>Blue layer is active when space is held.</appendix>
    <appendix>Red layer is active when return is held.</appendix>
    <identifier>private.rowup</identifier>

    <modifierdef>BLUE_SHIFT</modifierdef>
    <modifierdef>RED_SHIFT</modifierdef>
    <modifierdef>GREEN_SHIFT</modifierdef>

    <!-- MOD ROW -->
    <autogen>__KeyToKey__ KeyCode::Z, KeyCode::OPTION_L</autogen>

    <autogen>__KeyOverlaidModifier__ KeyCode::X,
      KeyCode::COMMAND_L, KeyCode::DELETE</autogen>
    <autogen>__KeyOverlaidModifier__ KeyCode::C,
      KeyCode::CONTROL_L, KeyCode::DELETE</autogen>
    <autogen>__KeyOverlaidModifier__ KeyCode::V,
      KeyCode::SHIFT_L, KeyCode::ESCAPE</autogen>

    <autogen>__KeyOverlaidModifier__ KeyCode::M,
      KeyCode::VK_MODIFIER_BLUE_SHIFT, KeyCode::SPACE</autogen>
    <autogen>__KeyOverlaidModifier__ KeyCode::COMMA,
      KeyCode::VK_MODIFIER_RED_SHIFT, KeyCode::RETURN</autogen>
    <autogen>__KeyOverlaidModifier__ KeyCode::DOT,
      KeyCode::VK_MODIFIER_GREEN_SHIFT, KeyCode::TAB</autogen>

    <!-- BLUE TOP ROW -->
    <autogen>__KeyToKey__ KeyCode::KEY_1, ModifierFlag::BLUE_SHIFT,
      KeyCode::BACKQUOTE</autogen>
    <autogen>__KeyToKey__ KeyCode::KEY_2, ModifierFlag::BLUE_SHIFT,
      KeyCode::KEY_2, ModifierFlag::SHIFT_L</autogen>
    <autogen>__KeyToKey__ KeyCode::KEY_3, ModifierFlag::BLUE_SHIFT,
      KeyCode::KEY_3, ModifierFlag::SHIFT_L</autogen>
    <autogen>__KeyToKey__ KeyCode::KEY_4, ModifierFlag::BLUE_SHIFT,
      KeyCode::KEY_4, ModifierFlag::SHIFT_L</autogen>
    <autogen>__KeyToKey__ KeyCode::KEY_5, ModifierFlag::BLUE_SHIFT,
      KeyCode::KEY_5, ModifierFlag::SHIFT_L</autogen>

    <autogen>__KeyToKey__ KeyCode::KEY_8, ModifierFlag::BLUE_SHIFT,
      KeyCode::KEY_6, ModifierFlag::SHIFT_L</autogen>
    <autogen>__KeyToKey__ KeyCode::KEY_9, ModifierFlag::BLUE_SHIFT,
      KeyCode::BRACKET_LEFT</autogen>
    <autogen>__KeyToKey__ KeyCode::KEY_0, ModifierFlag::BLUE_SHIFT,
      KeyCode::BRACKET_RIGHT</autogen>
    <autogen>__KeyToKey__ KeyCode::MINUS, ModifierFlag::BLUE_SHIFT,
      KeyCode::KEY_7, ModifierFlag::SHIFT_L</autogen>
    <autogen>__KeyToKey__ KeyCode::EQUAL, ModifierFlag::BLUE_SHIFT,
      KeyCode::BACKSLASH</autogen>

    <!-- BLUE MIDDLE ROW -->
    <autogen>__KeyToKey__ KeyCode::Q, ModifierFlag::BLUE_SHIFT,
      KeyCode::EQUAL, ModifierFlag::SHIFT_L</autogen>
    <autogen>__KeyToKey__ KeyCode::W, ModifierFlag::BLUE_SHIFT,
      KeyCode::KEY_8, ModifierFlag::SHIFT_L</autogen>
    <autogen>__KeyToKey__ KeyCode::E, ModifierFlag::BLUE_SHIFT,
      KeyCode::MINUS</autogen>
    <autogen>__KeyToKey__ KeyCode::R, ModifierFlag::BLUE_SHIFT,
      KeyCode::EQUAL</autogen>
    <autogen>__KeyToKey__ KeyCode::T, ModifierFlag::BLUE_SHIFT,
      KeyCode::MINUS, ModifierFlag::SHIFT_L</autogen>

    <autogen>__KeyToKey__ KeyCode::I, ModifierFlag::BLUE_SHIFT,
      KeyCode::KEY_1, ModifierFlag::SHIFT_L</autogen>
    <autogen>__KeyToKey__ KeyCode::O, ModifierFlag::BLUE_SHIFT,
      KeyCode::KEY_9, ModifierFlag::SHIFT_L</autogen>
    <autogen>__KeyToKey__ KeyCode::P, ModifierFlag::BLUE_SHIFT,
      KeyCode::KEY_0, ModifierFlag::SHIFT_L</autogen>
    <autogen>__KeyToKey__ KeyCode::BRACKET_LEFT, ModifierFlag::BLUE_SHIFT,
      KeyCode::QUOTE, ModifierFlag::SHIFT_L</autogen>
    <autogen>__KeyToKey__ KeyCode::BRACKET_RIGHT, ModifierFlag::BLUE_SHIFT,
      KeyCode::QUOTE</autogen>

    <!-- BLUE BOTTOM ROW -->
    <autogen>__KeyToKey__ KeyCode::A, ModifierFlag::BLUE_SHIFT,
      KeyCode::KEY_1</autogen>
    <autogen>__KeyToKey__ KeyCode::S, ModifierFlag::BLUE_SHIFT,
      KeyCode::KEY_2</autogen>
    <autogen>__KeyToKey__ KeyCode::D, ModifierFlag::BLUE_SHIFT,
      KeyCode::KEY_3</autogen>
    <autogen>__KeyToKey__ KeyCode::F, ModifierFlag::BLUE_SHIFT,
